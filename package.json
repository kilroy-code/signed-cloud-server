  "bugs": {
    "url": "https://github.com/kilroy-code/signed-cloud-server/issues"
  },
  "homepage": "https://github.com/kilroy-code/signed-cloud-server#readme",
  "devDependencies": {
    "@kilroy-code/distributed-security": "^0.0.4",
    "express": "^4.19.2",
    "jasmine": "^5.1.0",
    "morgan": "^1.10.0"
  }
